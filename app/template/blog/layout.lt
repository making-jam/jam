<div class="content">
	{? foreach($template["content"] as $k) { ?}
	<article class="flex column">
		<p>{?= $k["title"] =?}</p>
		<small>{?= $k["desc"] =?}</small>
		<div class="tags flex">
			{? foreach ($k["tags"] as $tag) { ?}
			<span class="label {?= $tag =?}">{?= $tag =?}</span>
			{? } ?}
		</div>
		<a href="{?= $k[" link"] =?}">View associated URL</a>
	</article>
	{? } ?}
</div>